{
    "name": "programic/laravel-admin-ui",
    "license": "proprietary",
    "type": "package",
    "description": "Laravel extension",
    "keywords": [
        "programic",
        "laravel"
    ],
    "readme": "README.md",
    "authors": [
        {
            "name": "Programic",
            "email": "development@programic.com",
            "homepage": "https://programic.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^5.6|^7.0|^8.0",
        "laravel/framework": "^5.0|^6.0|^7.0|^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Programic\\ProAdmin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Programic\\ProAdmin\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Programic\\ProAdmin\\ProAdminServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit",
        "check:ecs": [
            "@php vendor/bin/ecs --ansi check"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
